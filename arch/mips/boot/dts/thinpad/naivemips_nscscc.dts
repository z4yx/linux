/dts-v1/;

#include <dt-bindings/interrupt-controller/irq.h>

/ {
	compatible = "Tsinghua,NaiveMIPS,FPGA-A7";
	#address-cells = <1>;
	#size-cells = <1>;

	chosen {
		bootargs = "cca=0 console=ttyS0,115200n8 rdinit=/sbin/init uio_pdrv_genirq.of_id=lcd";
	};

	cpus {
		#address-cells = <1>;
		#size-cells = <0>;
		cpu@0 {
			device_type = "cpu";
			compatible = "mips,4Kc";
			clocks	= <&ext>;
			reg = <0>;
		};
	};

	ext: ext {
		compatible = "fixed-clock";
		#clock-cells = <0>;
	};

	memory {
		device_type = "memory";
		reg = <0x0 0x8000000>;
	};

	cpuintc: interrupt-controller@0 {
		#address-cells = <0>;
		#interrupt-cells = <1>;
		interrupt-controller;
		compatible = "mti,cpu-interrupt-controller";
	};
	
	soc{
		compatible = "simple-bus";
		#address-cells = <1>;
		#size-cells = <1>;
		#interrupt-cells = <1>;
		ranges;
		
		serial0: serial@0xbfd03000 {
			device_type = "serial";
			compatible = "xlnx,xps-uart16550-2.00.a", "ns16550a";
			reg = <0x1fd03000 0x1000>;
			reg-io-width = <4>;
			reg-shift = <2>;
			current-speed = <115200>;
			clock-frequency = <100000000>;
			interrupt-parent = <&cpuintc>;
			interrupts = <4>;
		};
		
		lcd0: lcd@0x1c060000 {
			compatible = "lcd";
			reg = <0x1c060000 0x1000>;
		};

		sw_led: gpio@0xbfd01000 {
			#gpio-cells = <2>;
			compatible = "xlnx,xps-gpio-1.00.a";
			gpio-controller ;
			reg = < 0x1fd01000 0x1000 >;
			xlnx,all-inputs = <0x0>;
			xlnx,all-inputs-2 = <0xffffffff>;
			xlnx,dout-default = <0x0>;
			xlnx,dout-default-2 = <0x0>;
			xlnx,gpio-width = <32>;
			xlnx,gpio2-width = <32>;
			xlnx,is-dual = <0x1>;
			xlnx,tri-default = <0x0>;
			xlnx,tri-default-2 = <0x0>;
		};
		axi_quad_spi@1e000000 {
			compatible = "xlnx,xps-spi-2.00.a";
			interrupt-parent = <&cpuintc>;
			interrupts = <3>;
			reg = <0x1e000000 0x10000>;
			xlnx,num-ss-bits = <0x1>;
		};
		altr_intc: altr_intc@1c070000 {
			compatible = "altr,pio-1.0";
			reg = <0x1c070000 0x1000>;
			altr,ngpio = <32>;
			resetvalue = <0>;
			#gpio-cells = <2>;
			gpio-controller;
			interrupt-parent = <&cpuintc>;
			interrupts = <2>;
			altr,interrupt-type = <IRQ_TYPE_EDGE_RISING>;
			interrupt-controller;
			#interrupt-cells = <2>;
		};
		axi_intc_0: interrupt-controller@1c080000 {
			#interrupt-cells = <2>;
			compatible = "xlnx,xps-intc-1.00.a";
			interrupt-controller;
			interrupt-parent = <&cpuintc>;
			interrupts = <5>;
			reg = <0x1c080000 0x1000>;
			xlnx,kind-of-intr = <0x1>;
			xlnx,num-intr-inputs = <0x1>;
		};
		axi_ethernetlite: ethernet@1c030000 {
			compatible = "xlnx,xps-ethernetlite-3.00.a";
			device_type = "network";
			local-mac-address = [08 86 4C 0D F7 09];
			phy-handle = <&phy0>;
			reg = <0x1c030000 0x10000>;
			xlnx,duplex = <0x1>;
			xlnx,include-global-buffers = <0x1>;
			xlnx,include-internal-loopback = <0x0>;
			xlnx,include-mdio = <0x1>;
			xlnx,instance = "axi_ethernetlite_inst";
			xlnx,rx-ping-pong = <0x1>;
			xlnx,s-axi-id-width = <0x1>;
			xlnx,tx-ping-pong = <0x1>;
			xlnx,use-internal = <0x0>;
			interrupt-parent = <&axi_intc_0>;
			interrupts = <0 IRQ_TYPE_EDGE_RISING>;
			mdio {
				#address-cells = <1>;
				#size-cells = <0>;
				phy0: phy@1 {
					device_type = "ethernet-phy";
					reg = <1>;
				} ;
			} ;
	    } ;
	};

	
};

&ext {
	clock-frequency = <40000000>;
};
